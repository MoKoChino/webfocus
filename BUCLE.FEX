-SET &ECHO=ALL;
TABLE FILE CAR
PRINT CAR 
WHERE RECORDLIMIT EQ 8
WHERE READLIMIT EQ 8
ON TABLE HOLD AS XXX
END
-SET &LINEASN = &LINES;
-IF &LINEASN EQ 0 THEN GOTO SAL;
-RUN
-REPEAT CICLO FOR 1 FROM 1 TO &LINEASN;
-READ XXX &COCHE.&CONT.A250. &COCHE.&CONT.A250.,
-CICLO
-RUN
-TYPE &COCHE.&CONT
-EXIT
TABLE FILE CAR
HEADING
"Este es el informe de modelos para &COCHE"
PRINT CAR MODEL
WHERE CAR EQ '&COCHE'
END
-SAL